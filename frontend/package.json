    "vitest": "vitest",
    "start:backend": "cd ../backend && uvicorn app.main:app --host 0.0.0.0 --port 8000"
  },
  "dependencies": {
    "@playwright/test": "^1.52.0",
    "axios": "^1.8.1",
    "bootstrap": "^5.3.3",  
