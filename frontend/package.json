    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.6",
    "cypress": "^14.0.3",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "start-server-and-test": "^2.0.10",
    "vite": "^6.2.6",
    "vite-plugin-vue-devtools": "^7.7.2",
