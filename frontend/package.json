{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "cypress install",
    "test:e2e": "npm run build && start-server-and-test preview http://localhost:5173 'cypress run --e2e'",
